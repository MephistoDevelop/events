<div class="container">
 <h1> <%= @attendances  %></h1>
  <%= form_for(:attendance, url: event_created_path) do |f| %>
      <% get_users_name.each do |item| %>
          <%= f.check_box :tag_ids,{multiple:true},item.email,nil %> <%= item.name %>
          <br />
      <% end %>
      <%= f.submit  "Invite", class: "btn btn-primary" %>
  <% end %>
</div>